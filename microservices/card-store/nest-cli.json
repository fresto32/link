{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "assets": [
    "/**/*.yml"
  ],
  "watchAssets": true,
  "projects": {
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}